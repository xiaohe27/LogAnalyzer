package rvm;

Pub(Integer report) {
  Integer report;
  long time;

  event publish (Integer report, long time) {
		this.report=report;
		this.time=time;		
	} 

  event approve (Integer report, long time) {
		this.report=report;
		this.time=time;		
	}

  ltl: [](publish => (*) approve)

  @violation { System.out.println("should not publish financial report "+this.report+" without pre-approval");}
 
}
